          <argument>
            <kind>1</kind>
            <string>///here we animate the Logo
white_screen = 0;
dark_blue_square = 1;
light_blue_square = 2;
hyky_loop = 3;
fade_out = 4;
white_wait = 5; 


//mmmhhh... which animation step do we have right now and is it over?
if(animation_step==white_screen &amp;&amp; animation_ticker&gt;120){
    animation_step++;
    image_alpha=1.0;
    screen_shake_timer=15;
    animation_ticker=0;
    if(os_browser!=browser_not_a_browser){
            sound_play(snd_fumm);
        }else{
            audio_play_sound(snd_fumm, 10, false);
        }
}else if(animation_step==dark_blue_square &amp;&amp; animation_ticker&gt;60){
    animation_step++;
    image_index++;
    screen_shake_timer=15;
    animation_ticker=0;
    if(os_browser!=browser_not_a_browser){
            sound_play(snd_fumm);
        }else{
            audio_play_sound(snd_fumm, 10, false);
        }
}else if(animation_step==light_blue_square &amp;&amp; animation_ticker&gt;60){
    animation_step++;
    image_index++;
    animation_ticker=0;
    screen_shake_timer=15;
    animation_ticker=0;
    if(os_browser!=browser_not_a_browser){
            sound_play(snd_fch);
        }else{
            audio_play_sound(snd_fch, 10, false);
        }
}else if((animation_step==hyky_loop || animation_step==fade_out) &amp;&amp; animation_ticker%5==0){
    image_index++;
    if(image_index &gt; 5){
        image_index=2;
    }
    if(animation_ticker &gt; 300){
        animation_step++;
        animation_ticker=0;
        if(browser_not_a_browser){
            sound_fade(snd_fch, 0, 500);
        }else{
            audio_sound_gain(snd_fch, 0, 500);
        }
    }
}else if(animation_step==fade_out){
    with(instance_find(obj_blackscreen,0)){
        isInChange = true;
    }
}

//quit intro button
if((keyboard_check(vk_return) || keyboard_check(vk_space) || keyboard_check(vk_escape) || mouse_check_button_pressed(mb_left))){
    animation_step = fade_out;
    if(browser_not_a_browser){
        sound_fade(snd_fch, 0, 500);
    }else{
        audio_sound_gain(snd_fch, 0, 500);
    }
}

//Screenshake
if(screen_shake_timer&gt;0){
    screen_shake_timer--;
    x=540 + random(screen_shake_timer*2)-screen_shake_timer;
    y=960 + random(screen_shake_timer*2)-screen_shake_timer;
}

//at last we add one to our ticker
animation_ticker++;
</string>
