            <string>///Initiates variables needed for animation

//which step we have currently
animation_step = 0;

//a ticker that helps us to time the next animation step
animation_ticker = 0;

//we do that by our own. :0)
image_speed = 0;

//set the image unvisible
image_alpha = 0;

//set shake timer to zero
screen_shake_timer = 0;

//load old highscores
highscore_clear();

ini_open("highscore.sav");

for(var i=0;i&lt;=10;i++){
    highscore_add("", ini_read_real("Highscore", "score", 0));
    global.dots = ini_read_real("Highscore", "dots", 0);
    global.time = ini_read_real("Highscore", "time", 0);
    global.tutorial = ini_read_real("TUTORIAL", "show", true);
}

ini_close();

global.lastColorType = 0;
</string>
